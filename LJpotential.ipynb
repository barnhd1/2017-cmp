   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def LJ(r):\n",
    "    r3 = r**3\n",
    "    r6 = r3*r3\n",
    "    return 4*(1/r6 - 1/r3)\n",
    "\n",
    "r = np.linspace(1.0,5.0,100)\n",
    "\n",
    "plt.title(\"LJ Potential N = 3\")\n",
    "plt.plot(r, LJ(r))\n",
    "plt.show()\n",
    "\n",
    "print('minimum value:   ', min(LJ(r)))"
   ]
